# Teamloop Environment Configuration
# Official Teamloop Logo: https://automationalien.s3.us-east-1.amazonaws.com/teamloop_logo_2.png

# Backend API URL
VITE_API_BASE_URL=http://localhost:3001

# Supabase Configuration (for database - pending PM decision)
VITE_SUPABASE_URL=your_supabase_project_url
VITE_SUPABASE_ANON_KEY=your_supabase_anon_key

# OpenAI Configuration (for GPT and Whisper)
VITE_OPENAI_API_KEY=your_openai_api_key

# ElevenLabs Configuration (for enhanced TTS)
VITE_ELEVENLABS_API_KEY=your_elevenlabs_api_key
VITE_ELEVENLABS_VOICE_ID=your_preferred_voice_id

# Teamloop Specific Configuration
VITE_APP_NAME=Teamloop
VITE_APP_VERSION=1.0.0
VITE_APP_ENVIRONMENT=development

# Feature Flags
VITE_ENABLE_VOICE_AI=true
VITE_ENABLE_AI_CHAT=true
VITE_ENABLE_ELEVENLABS_TTS=true
VITE_ENABLE_WHISPER_TRANSCRIPTION=true
